@{
    ViewData["Title"] = "Upload Page";
}
@using (Html.BeginForm("Upload", "Home", null, FormMethod.Post, null, new { enctype = "multipart/form-data" }))
{

    @Html.AntiForgeryToken()
    <div class="Upload">
        <h1>Upload your file</h1>
        <input type="file" id="upload" name="upload" accept=".csv" required /><br>
        <input type="submit" value="Upload" class="btn btn-default" />
    </div>
}